en einen Champion einsetzt; Annie stirbt.<br><br><span class=\"colorFFEB7F\">Erzürnt</span>: Tibbers bekommt zusätzlich 275&nbsp;% Angriffstempo und 100&nbsp;% Lauftempo. Diese Boni klingen über 3 Sekunden wieder ab.","leveltip":{"label":["Schaden","Tibbers' Leben","Tibbers' Rüstung und Magieresistenz","Tibbers' Angriffsschaden","Tibbers' Feuerschaden","Abklingzeit"],"effect":["{{ e1 }} -> {{ e1NL }}","{{ e10 }} -> {{ e10NL }}","{{ e7 }} -> {{ e7NL }}","{{ e3 }} -> {{ e3NL }}","{{ e2 }} -> {{ e2NL }}","{{ cooldown }} -> {{ cooldownnNL }}"]},"maxrank":3,"cooldown":[120,100,80],"cooldownBurn":"120/100/80","cost":[100,100,100],"costBurn":"100","effect":[null,[150,275,400],[10,15,20],[50,75,100],[-0.1,-0.1,-0.1],[1,1,1],[3,3,3],[30,50,70],[0,900,1800],[0.15,0.15,0.15],[1200,2100,3000]],"effectBurn":[null,"150/275/400","10/15/20","50/75/100","-0.1","1","3","30/50/70","0/900/1800","0.15","1200/2100/3000"],"vars":[{"link":"spelldamage","coeff":0.65,"key":"a1"},{"link":"spelldamage","coeff":0.1,"key":"a2"}],"costType":"Mana","maxammo":"-1","range":[600,600,600],"rangeBurn":"600","image":{"full":"InfernalGuardian.png","sprite":"spell1.png","group":"spell","x":48,"y":0,"w":48,"h":48},"resource":"{{ cost }} Mana"}],"passive":{"name":"Pyromanie","description":"Nach jeweils 4 gewirkten Zaubern wird das Ziel durch Annies nächsten offensiven Zauber für kurze Zeit betäubt.","image":{"full":"Annie_Passive.png","sprite":"passive0.png","group":"passive","x":288,"y":0,"w":48,"h":48}},"recommended":[{"champion":"Annie","title":"Map12","map":"HA","mode":"ARAM","type":"riot","customTag":"","sortrank":0,"extensionPage":false,"customPanel":null,"blocks":[{"type":"starting","recMath":false,"recSteps":false,"minSummonerLevel":-1,"maxSummonerLevel":-1,"showIfSummonerSpell":"","hideIfSummonerSpell":"","items":[{"id":"3112","count":1,"hideCount":false},{"id":"1001","count":1,"hideCount":false},{"id":"2031","count":1,"hideCount":false}]},{"type":"essential","recMath":false,"recSteps":false,"minSu